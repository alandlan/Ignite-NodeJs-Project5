{
  "compilerOptions": {
  "target": "es5",
  "module": "commonjs",
  "outDir": "./dist",
  "strict": true, 
  "resolveJsonModule": true,
  "baseUrl": "./src", 
  "paths": {
    "@modules/*": ["modules/*"],
    "@config/*":["config/*"],
    "@shared/*":["shared/*"],
    "@errors/*":["errors/*"],
    "@utils/*": ["utils/*"]
  },
  "esModuleInterop": true,
  "experimentalDecorators": true,  
  "emitDecoratorMetadata": true,   
  "forceConsistentCasingInFileNames": true,
  "sourceMap": true
  }
}
